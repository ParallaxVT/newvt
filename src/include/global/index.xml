<krpano>

  <events onpreviewcomplete="if(develmode.enabled!=true,delayedcall(5, set(autorotate.enabled,true)));"/>

  <addglobal enabled="true"/>

  <display fps="30"
           details="20"
           tessmode="0"
           movequality="low"
           stillquality="high"
           flash10="on"
           movequality10="best"
           stillquality10="best"
           stilltime="0.25"
           />

  <autorotate enabled="false" waittime="7" horizon="off" speed="6"/>

  <progress showload="none" showwait="none" showreloads="false" parent="" />

  <control mousefovchange="0"/>

  <data name="custom_fov">100</data>

  <view hlookat="0"
        vlookat="0"
        fov="100"
        fovmin="25"
        fovmax="110"
        limitfov="true"
        fovtype="DFOV"
        maxpixelzoom="1"
        />

  <network downloadqueues="6" decodequeues="2"/>

  <security>
    <crossdomainxml url="[SETCROSSDOMAIN]" />
  </security>

  <contextmenu>
    <item name="build"
          caption="Built by TourVista"
          onclick="openurl(http://www.tourvista.co.uk,_blank);"
          />
  </contextmenu>

</krpano>
